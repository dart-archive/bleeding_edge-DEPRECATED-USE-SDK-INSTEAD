#
# Internal options for the Dart Compiler
#
# To run Dart Editor with custom options,
# copy one or more of these settings to an options file
# then launch Dart Editor from the command line with 
#
#     -debug /path/to/options/file
#
com.google.dart.tools.core/debug/build = false
com.google.dart.tools.core/debug/dartlib = false
com.google.dart.tools.core/debug/index/contributor = false
com.google.dart.tools.core/debug/metrics = false
com.google.dart.tools.core/debug/warmup = false
com.google.dart.tools.core/debug/verbose = false
com.google.dart.tools.core/logging/frog = false
com.google.dart.tools.core/logging/debugger = false

com.google.dart.tools.core/perf/analysis/filescan = false

com.google.dart.tools.core/experimental/analysis/server = false
com.google.dart.tools.core/experimental/callHierarchy = false
com.google.dart.tools.core/experimental/serverDebugging = false
com.google.dart.tools.core/experimental/update = false

com.google.dart.tools.core/trace/artifactProvider = false
com.google.dart.tools.core/trace/index/contributor = false
com.google.dart.tools.core/trace/index/processor = false

com.google.dart.tools.core/perf/index = false

