part of $LIBRARYNAME;

@DocsEditable()
$(ANNOTATIONS)$(CLASS_MODIFIERS)class $CLASSNAME$EXTENDS$MIXINS$IMPLEMENTS$NATIVESPEC {
$!MEMBERS

    /// Checks if HTML imports are supported on the current platform.
  bool get supportsImport {
