# Don't run any test-like files that show up in packages directories. It
# shouldn't be necessary to run "pub install" in these packages, but if you do
# it shouldn't break the tests.
*/packages/*/*: Skip
*/*/packages/*/*: Skip
*/*/*/packages/*/*: Skip
*/*/*/*/packages/*/*: Skip
*/*/*/*/*/packages/*/*: Skip

[ $runtime == vm ]
dart_style/test/command_line_test: Fail # Issue 22585

[ $compiler == dart2dart ]
dart_style/test/formatter_test: Fail # Issue 22587

[ $browser ]
*: SkipByDesign
